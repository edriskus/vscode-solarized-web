{
    "name": "solarized-light-web",
    "displayName": "Solarized Web",
    "description": "Web Solarized themes for vs-code",
    "version": "3.1.1",
    "publisher": "edriskus",
    "engines": {
        "vscode": "*"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/edriskus/vscode-solarized-web/tree/master"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Solarized Web Light",
                "uiTheme": "vs",
                "path": "./themes/solarized-custom-light.json"
            },
            {
                "label": "Solarized Web Light Italic",
                "uiTheme": "vs",
                "path": "./themes/solarized-custom-light-italic.json"
            },
            {
                "label": "Solarized Web Dark",
                "uiTheme": "vs-dark",
                "path": "./themes/solarized-custom-dark.json"
            },
            {
                "label": "Solarized Web Dark Italic",
                "uiTheme": "vs-dark",
                "path": "./themes/solarized-custom-dark-italic.json"
            },
            {
                "label": "Solarized Web Extra Dark",
                "uiTheme": "vs-dark",
                "path": "./themes/solarized-custom-extra-dark.json"
            },
            {
                "label": "Solarized Web Extra Dark Italic",
                "uiTheme": "vs-dark",
                "path": "./themes/solarized-custom-extra-dark-italic.json"
            }
        ]
    },
    "icon": "icon.png",
    "dependencies": {},
    "devDependencies": {
        "del": "^3.0.0",
        "gulp": "^4.0.0",
        "gulp-file": "^0.4.0",
        "gulp-jsbeautifier": "^3.0.0",
        "gulp-json-editor": "^2.5.0",
        "gulp-json5-to-json": "^0.2.1",
        "gulp-merge-json": "^1.3.1",
        "gulp-rename": "^1.4.0"
    }
}
